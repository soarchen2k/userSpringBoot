<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="ca.monor.dao.UserDao">

    <select id="queryAllUser" resultType="ca.monor.entity.User">
		SELECT
        *
		FROM
		user
		ORDER BY
		id
	</select>
</mapper>